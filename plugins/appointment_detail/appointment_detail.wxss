/* pages/detail/detail.wxss */
.header{
  width: 100%;
  height: 160px;
}
.header image{
  width: 100%;
  height: 160px;
  display: block;
  background-size: 100%;
}
.order{
    padding-left: 8px;
    padding-right: 8px;                                                     
    width: 100%;
    border-bottom: 1px solid #e4e4e4
}
.order-name{
    width: 100%;
    height: 20px;
    line-height: 20px;
    font-size: 15px;
    margin-top: 8px;
    margin-bottom: 8px;
}
.order-detail{
    overflow: hidden;
    text-overflow: ellipsis;
    display: -webkit-box;
    -webkit-box-orient: vertical;
    margin-bottom: 5px;
    font-size: 12px;
    padding-right: 18px;
    flex-shrink: 0;
    width: 100%;
    transition: opacity .3s;
    opacity: 1;
    color: #657180;
    }
    .order-num{
      font-size: 12px;
      width: 100%;
      height: 30px;
      line-height: 30px;
      color: #b5b5b5;
      display: flex;
      flex-flow: nowrap;
      justify-content:space-around
      }
.order-num-o{padding-right: 14px;}
.order-num-span{color: #ff0036}
.ge{width: 100%;height:8px;background:#e4e4e4;margin-top: 10px}
.money-time{width: 100%;height: 30px;line-height: 30px;text-align: center;font-size: 13px; color: #b5b5b5}
.money-market{text-decoration:line-through;}
.money-time-M{font-size: 18px;color: #ff2349;margin-left: 8px;margin-right: 8px;}

.assess{
  height: 90px;
  width: 100%;
  text-align: center;
    }
.assess1{    
  height: 40px;
  width: 100%;
  text-align: center;
    }

.content{
    width: 100%;
    border-bottom: 1px solid #e4e4e4;
    font-size: 12px;
    color: #657180;
}

.content-T{
    width: 100%;
    height: 45px;
    }
.content-l{
    width: 100%;
    height: 45px;
    display: flex;
    padding: 5px
    }
.content-header{
  width: 50px;
  height: 50px;
  display: inline-block;
  vertical-align: middle;
}

.assess-m{font-size: 16px;color: #707070;margin-top: 15px;}
.assess-d{font-size: 13px;color: #b5b5b5;width:100%;height: 30px;line-height: 30px;}
.assess-d text{display:inline-block;vertical-align: middle}
.assess-d image{width: 12px;height: 12px;display:inline-block;vertical-align: middle}
.content-r{display: flex;flex-direction: column;margin-left: 8px;vertical-align: middle}
.content-date{
  height: 20px;
  line-height: 20px;
  width: 160px;
}
.content-title{
  width: 160px;
  height: 20px;
  line-height: 20px;
  color: rgb(102, 102, 102);
  font-size: 14px;
  font-weight: bold;
  font-style: normal;
  text-decoration: none;
}
.content-B{    
    width: 100%;
    height: 35px;
    padding-left: 2px;
    overflow: hidden;
    text-overflow: ellipsis;
    display: -webkit-box;
    -webkit-line-clamp: 2;
    -webkit-box-orient: vertical;
    color: #657180;
    margin-top: 20px;
    }
.detail{
    width: 100%;
    border: 1px solid #f5f5f5;
    font-size: 14px;
    color: #657180;
    margin-bottom: 46px;
    padding: 5px;
}
.more{
  width: 100px;
  height: 30px;
  border-radius: 5px;
  color: #b5b5b5;
  font-size: 12px;
  margin: 0 auto;
  border:2px solid #f5f5f5;
  line-height: 30px;
  text-align: center;
  margin-top: 20px;
}
.assess-t{font-size: 16px;color: #707070;margin-top: 15px;border-top:10px solid #f5f5f5}
.footer{    
  background: #ffffff;
  position: fixed;
  bottom: 0;
  height: 45px;
  width: 100%;
  display: flex;
  flex-wrap: nowrap;
  flex: 1;
  z-index: 8
  }
.footer-index{
      padding-top: 5px;
      flex: 1;
      height: 45px;
      text-align: center;
      border-top:1px solid #e5e5e5;

}


.footer-phone{
  flex: 1;
  height: 45px;
  text-align: center;
  padding-top: 5px;
  border-left:1px solid #e5e5e5;
    border-top:1px solid #e5e5e5
  }
.footer-btn{
      flex: 4;
      height: 45px;
      line-height: 45px;
      text-align: center;
      font-size: 13px;
}
.footer-btn button.chooseSezi{
     background: #ff0036;
      color: #ffffff;
}

.commodity_screen {
 width: 100%;
 height: 100%;
 position: fixed;
 top: 0;
 left: 0;
 opacity: 0.4;
 overflow: hidden;
 z-index: 999999;
 color: #fff;
 background: black
}

.commodity_attr_box {
 width: 100%;
 overflow: hidden;
 position: fixed;
 bottom: 0;
 left: 0;
 z-index: 999999999999999;
 background: #ffffff
}
.slide-header{
  text-align:center;

}
.slide-image{
  width: 100px;
  height: 100px;
  border-radius:50%;
  }
  .selecttime{
    width: 100%;
    margin-top: 20px;
  }
.selecttime-T{
  height: 50px;
  width: 390px;
  margin-top: 10px;
  font-size: 12px;
  color:#626262;
  margin: 0 auto;
  }
 .num-tt{
   width: 100%;
   height: 50px;
   border-bottom: 1px solid #e5e5e5;
   border-top: 1px solid #e5e5e5;
   margin-top: 15px;
   font-size: 12px;
  }
 .num-tt text{
   float: left;
   text-align: center;
   padding: 8px;
   line-height: 18px;
   }
.inline-block{
  display: inline-block
}
.box{
  display:flex;
  flex-wrap: wrap;
  font-size: 13px;
  margin-top: 10px;
  margin-bottom: 50px;}

.item{
  width:25%;
  box-sizing: border-box;
  }
.product-item{
  height: 60px;
  }

.product-item text{
  border: 1px solid red;
  width: 90px;
  height: 50px;
  line-height: 50px;
  text-align: center;
  display: block;
  margin: 0 auto;
  background: #ffeef0;
  color: #a6a5a5;
}
.product-item.active text{
  background: #ff2349;
  color: #fff;
}
.product-date.active text{
  background: #ff2349;
  color: #fff;
}
.footer1{
  position: fixed;
  bottom: 0;
  width: 100%;
}
.footer1 button,.footer button{
  border: 0;
  width: 100%;
  height: 45px;
  line-height: 45px;
  text-align: center;
  background: #ff0036;
  color: #ffffff
}

.header-t{
      width: 100%;
      height: 30px;
      line-height: 30px;
      text-align: center;
      border-bottom: 1px solid #e5e5e5;
}
.header-t image{
        width: 14px;
        height: 14px;
        display: inline-block;
        position: absolute;
        margin-top: 8px;
        left: 2px;
}
.header-t text{font-size: 13px;color: #434343}


